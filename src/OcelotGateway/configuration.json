{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/sso/api/{everything}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/sso/api/{everything}",
      "UpstreamHttpMethod": [ "Get" ],
      "ServiceName": "SSO",
      "LoadBalancer": "LeastConnection",
      "UseServiceDiscovery": true
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "localhost",
      "Port": 8500
    }
  }
}